    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.1",
    "whatwg-fetch": "1.0.0"
  },
  "dependencies": {
    "blob-util": "1.2.1",
    "bootstrap": "3.3.7",
    "bootstrap-select": "1.13.1",
    "bootstrap-switch": "3.3.4",
    "connected-react-router": "4.3.0",
    "history": "4.7.2",
    "immutable": "3.8.2",
    "intl-messageformat": "2.2.0",
    "jquery": "=3.2.1",
    "moment": "2.22.2",
    "moment-duration-format": "2.2.2",
    "object-assign": "4.1.1",
    "patternfly": "3.53.1",
    "patternfly-bootstrap-combobox": "1.1.7",
    "patternfly-react": "2.11.1",
    "popper.js": "1.14.3",
    "prop-types": "15.6.1",
    "react": "16.4.0",
    "react-bootstrap": "0.32.1",
    "react-bootstrap-switch": "15.5.3",
    "react-dom": "16.4.0",
    "react-infinite-scroller": "1.1.4",
    "react-intl": "2.4.0",
    "react-intl-po": "2.2.2",
    "react-redux": "5.0.7",
    "react-router": "4.2.0",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.2.2",
    "react-router-navigation-prompt": "1.6.3",
    "react-test-renderer": "16.4.0",
    "redux": "4.0.0",
    "redux-saga": "0.16.0"
  },
  "resolutions": {
    "redux": "4.0.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "eslint": "eslint -c config/eslint.js src",
    "eslint:fix": "eslint -c config/eslint.js src --fix",
    "prebuild": "yarn run test",
    "test": "jest --no-watchman",
    "test:watch": "jest --watch --no-watchman",
    "intl:extract": "babel-node scripts/intl/extract-messages.js --presets env,flow && rip json2pot -o extra/to-zanata/messages.pot extra/to-zanata/messages.json",
    "intl:push": "zanata-cli push",
    "intl:pull": "zanata-cli pull -e",
    "intl:apply": "rip po2json -o extra/from-zanata/translated-messages.json -m extra/to-zanata/messages.json 'extra/from-zanata/*.po' && babel-node scripts/intl/normalize-messages.js --presets env,flow"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/FileStub.js",
      "^.+\\.css$": "<rootDir>/config/jest/CSSStub.js",
      "ovirtapi": "<rootDir>/src/mock/ovirtapi.mock.js",
      "app-intl": "<rootDir>/src/intl/index.js",
      "app-actions": "<rootDir>/src/actions/index.js",
      "app-reducers(.*)$": "<rootDir>/src/reducers/$1",
      "app-constants": "<rootDir>/src/constants/index.js",
      "app-helpers": "<rootDir>/src/helpers.js"
    },
    "transform": {
      ".*": "<rootDir>/config/jest/transform.js"
    },
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/jest/chalk.logger.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules|__tests__/helper|config)/"
    ],
    "testEnvironment": "jsdom",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx)$",
    "testURL": "http://localhost/"
  },
  "eslintConfig": {
    "extends": "./config/eslint.js"
  },
  "productionHomepage": "/ovirt-engine/web-ui/"
}
